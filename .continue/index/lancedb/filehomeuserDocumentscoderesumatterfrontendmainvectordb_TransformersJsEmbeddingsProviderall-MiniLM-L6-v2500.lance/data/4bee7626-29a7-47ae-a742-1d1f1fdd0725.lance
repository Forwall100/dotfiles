<script>
  import { onMount, onDestroy } from 'svelte';
  import { goto } from '$app/navigation';
  import { supabase } from '$lib/supabaseClient';
  import { credits } from '$lib/stores/authStore';
  import { LoaderCircle } from 'lucide-svelte';

  let isLoading = true;
  let errorMessage = '';
  let paymentId = '';
  let checkInterval;

  async function checkPaymentStatus() {
    try {
      const { data: { session } } = await supabase.auth.getSession();
      if (!session) {
        goto('/login');
        return;
      }

      const response = await fetch('/api/check-payment', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
          'Authorization': `Bearer ${session.access_token}`
        },
        body: JSON.stringify({ payment_id: paymentId })
      });

      const data = await response.json();

      if (!response.ok) {
        throw new Error(data.error || '–ü—Ä–æ–∏–∑–æ—à–ª–∞ –æ—à–∏–±–∫–∞ –ø—Ä–∏ –ø—Ä–æ–≤–µ—Ä–∫–µ –ø–ª–∞—Ç–µ–∂–∞');
      }

      if (data.status === 'success') {
        credits.set(data.credits);
        clearInterval(checkInterval);
        goto('/dashboard');
      } else if (data.status === 'canceled' || data.status === 'refunded') {
        clearInterval(checkInterval);
        errorMessage = '–ü–ª–∞—Ç–µ–∂ –±—ã–ª –æ—Ç–º–µ–Ω–µ–Ω. –ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞.';
        isLoading = false;
      }
    } catch (error) {
      console.error('Error checking payment status:', error);
      errorMessage = error.message;
      isLoading = false;
    }
  }

  onMount(async () => {
    const urlParams = new URLSearchParams(window.location.search);
    paymentId = urlParams.get('payment_id');
    
    if (!paymentId) {
      errorMessage = '–ù–µ —É–¥–∞–ª–æ—Å—å –ø–æ–ª—É—á–∏—Ç—å –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä –ø–ª–∞—Ç–µ–∂–∞';
      isLoading = false;
      return;
    }

    // –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å—Ç–∞—Ç—É—Å –ø–ª–∞—Ç–µ–∂–∞ –∫–∞–∂–¥—ã–µ 5 —Å–µ–∫—É–Ω–¥
    checkInterval = setInterval(checkPaymentStatus, 5000);
    // –ü–µ—Ä–≤–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ —Å—Ä–∞–∑—É
    await checkPaymentStatus();
  });

  onDestroy(() => {
    if (checkInterval) {
      clearInterval(checkInterval);
    }
  });
</script>

