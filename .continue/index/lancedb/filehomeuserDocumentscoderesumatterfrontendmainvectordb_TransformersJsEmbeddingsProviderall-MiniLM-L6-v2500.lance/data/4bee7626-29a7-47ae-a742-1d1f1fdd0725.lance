import { redirect } from '@sveltejs/kit';
import { supabase } from '$lib/supabase';

export async function load() {
  const { data } = await supabase.auth.getSession();
  
  if (!data.session) {
    throw redirect(302, '/login');
  }
  
  return {
    session: data.session
  };
}<!-- src/routes/dashboard/+layout.svelte -->
<script>
    import { user, credits, signOut } from "$lib/stores/authStore";
    import { goto } from "$app/navigation";
    import { onMount } from "svelte";
    import { Button } from "$lib/components/ui/button";
    import {
        Avatar,
        AvatarImage,
        AvatarFallback,
    } from "$lib/components/ui/avatar";
    import {
        DropdownMenu,
        DropdownMenuContent,
        DropdownMenuItem,
        DropdownMenuTrigger,
    } from "$lib/components/ui/dropdown-menu";
    import {
        Upload,
        LoaderCircle,
        FileText,
        Mail,
        Sparkle,
        Settings,
        Menu,
        X,
        ChevronRight,
        ChevronLeft,
    } from "@lucide/svelte";
    import logoUrl from "$lib/assets/logow.svg";
    import { browser } from "$app/environment";
    import { writable } from "svelte/store";

    let activeSection = "resume";

    let isSidebarCollapsed = false;

    let isMobileMenuOpen = false;

    onMount(async () => {
        if (!$user) {
            goto("/login");
        }

        if (browser) {
            const savedState = localStorage.getItem("sidebarCollapsed");
            if (savedState) {
                isSidebarCollapsed = savedState === "true";
            }

            // Add responsive handler for mobile
            const handleResize = () => {
                if (window.innerWidth < 768) {
                    isMobileMenuOpen = false;
                    // On mobile, don't auto-collapse, just hide completely and show via menu button
                }
            };

            handleResize(); // Run once on mount
            window.addEventListener("resize", handleResize);

            return () => {
                window.removeEventListener("resize", handleResize);
            };
        }
    });

    // Handle logout
    async function handleLogout() {
        await signOut();
        goto("/login");
    }

    // Toggle sidebar collapse state
    function toggleSidebar() {
        isSidebarCollapsed = !isSidebarCollapsed;
        if (browser) {
            localStorage.setItem(
                "sidebarCollapsed",
                isSidebarCollapsed.toString(),
            );
        }
    }

    // Toggle mobile menu
    function toggleMobileMenu() {
        isMobileMenuOpen = !isMobileMenuOpen;
    }

    // Get user initials for avatar fallback
    function getUserInitials() {
        if (!$user || !$user.user_metadata || !$user.user_metadata.full_name) {
            return "U";
        }
        const fullName = $user.user_metadata.full_name;
        const nameParts = fullName.split(" ");
        if (nameParts.length >= 2) {
            return `${nameParts[0][0]}${nameParts[1][0]}`;
        }
        return fullName[0] || "U";
    }

    // Get display name
    function getDisplayName() {
        if (!$user) return "User";
        if ($user.user_metadata && $user.user_metadata.full_name) {
            return $user.user_metadata.full_name;
        }
        return $user.email.split("@")[0] || "User";
    }
</script>

