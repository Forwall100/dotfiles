  // Handle hover effects for resume improvement cards
  onMount(() => {
    if (browser) {
      // Function to set up hover effects after typing is complete
      const setupHoverEffects = () => {
        setTimeout(() => {
          // Get all hover sections from the optimized resume
          const resumeContent = document.getElementById('optimized-resume-content');
          if (!resumeContent) return;
         
          // Find all hover sections and add direct event listeners
          const sections = resumeContent.querySelectorAll('.hover-section');
          
          sections.forEach(section => {
            // Extract the card ID from the data attribute or custom event
            const cardId = section.getAttribute('data-card') || 
                          section.getAttribute('onmouseover')?.match(/detail: '([^']+)'/)?.at(1);
                          
            if (cardId) {
              // Remove the inline event handlers and use proper event listeners
              section.removeAttribute('onmouseover');
              section.removeAttribute('onmouseout');
              
              // Add proper event listeners
              section.addEventListener('mouseenter', () => {
                console.log('Mouse enter:', cardId);
                activeCard = cardId;
              });
              section.addEventListener('mouseleave', () => {
                console.log('Mouse leave:', cardId);
                activeCard = null;
              });
            }
          });
        }, 1500); // Give more time for the DOM to update
      };
      // Watch for changes in typingInProgress
      let watchInterval = setInterval(() => {
        if (!typingInProgress && typedText.length > 0) {
          console.log('Typing completed, setting up hover effects');
          setupHoverEffects();
          clearInterval(watchInterval);
        }
      }, 500);
    }
  });
