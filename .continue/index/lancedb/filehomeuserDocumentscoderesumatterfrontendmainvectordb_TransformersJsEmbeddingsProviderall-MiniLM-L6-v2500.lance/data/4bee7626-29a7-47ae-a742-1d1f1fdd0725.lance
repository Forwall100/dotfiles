  function typeText() {
    if (currentPosition < optimizedResumeContent.length) {
      typingInProgress = true;
      const charsToType = Math.min(10, optimizedResumeContent.length - currentPosition);
      typedText = optimizedResumeContent.substring(0, currentPosition + charsToType);
      currentPosition += charsToType;
      setTimeout(typeText, 1);
    } else {
      typingInProgress = false;
    }
  }

  function startOptimization() {
    showOptimizedResume = true;
