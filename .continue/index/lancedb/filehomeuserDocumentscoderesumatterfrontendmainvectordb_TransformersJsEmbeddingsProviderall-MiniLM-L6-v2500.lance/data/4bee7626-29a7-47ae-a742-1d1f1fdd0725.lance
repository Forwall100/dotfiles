  import { Textarea } from "$lib/components/ui/textarea";
  import * as Alert from "$lib/components/ui/alert";
  import { ArrowLeft, Download, Save } from "@lucide/svelte";

  // Alert state
  let alertOpen = false;
  let alertTitle = "";
  let alertDescription = "";
  let timeoutId;

  function showAlert(title, description) {
    // Reset if already open
    clearTimeout(timeoutId);
    alertTitle = title;
    alertDescription = description;
    alertOpen = false;
    // Allow reflow then open
    requestAnimationFrame(() => (alertOpen = true));
    // –ó–∞–∫—Ä—ã—Ç—å —á–µ—Ä–µ–∑ 3 —Å–µ–∫—É–Ω–¥—ã
    timeoutId = window.setTimeout(() => closeAlert(), 3000);
  }

  function closeAlert() {
    clearTimeout(timeoutId);
    alertOpen = false;
  }

  export let data;
  const activityId = data.id;
  let selectedTemplate = "classic";

  const templates = [
    { value: "classic", label: "Classic", color: undefined },
    { value: "blue", label: "Blue", color: undefined },
    { value: "modern", label: "Modern", color: undefined },
  ];

  // –ò—Å—Ö–æ–¥–Ω—ã–π –∏ —Ä–µ–¥–∞–∫—Ç–∏—Ä—É–µ–º—ã–π —Ä–µ–∑—é–º–µ
  const originalResume = JSON.stringify(data.resume);
  let editableResume = JSON.parse(JSON.stringify(data.resume));

  // –°–æ—Å—Ç–æ—è–Ω–∏–µ –∏–∑–º–µ–Ω–µ–Ω–∏–π
  let hasChanges = false;

  function updateResume(mutator) {
    mutator();
    editableResume = JSON.parse(JSON.stringify(editableResume));
    // –ü—Ä–æ–≤–µ—Ä–∏—Ç—å –Ω–∞–ª–∏—á–∏–µ –∏–∑–º–µ–Ω–µ–Ω–∏–π
    hasChanges = JSON.stringify(editableResume) !== originalResume;
  }

  async function saveResume() {
    if (!hasChanges) return;
    const { error } = await supabase
      .from("activities")
      .update({ result: editableResume })
      .eq("id", activityId);

    if (error) {
      showAlert("–û—à–∏–±–∫–∞ –ø—Ä–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–∏", error.message);
    } else {
      showAlert("–£—Å–ø–µ—Ö", "–†–µ–∑—é–º–µ —É—Å–ø–µ—à–Ω–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–æ");
      // –°–±—Ä–æ—Å–∏—Ç—å —Ñ–ª–∞–≥ –∏–∑–º–µ–Ω–µ–Ω–∏–π
      hasChanges = false;
      // –û–±–Ω–æ–≤–∏—Ç—å –æ—Ä–∏–≥–∏–Ω–∞–ª
      Object.assign(originalResume, JSON.stringify(editableResume));
    }
  }

  function downloadResume() {
    const resumeContainer = document.querySelector(".resume-container");
    if (!resumeContainer) {
      alert("–ù–µ —É–¥–∞–ª–æ—Å—å –Ω–∞–π—Ç–∏ –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä —Å —Ä–µ–∑—é–º–µ");
      return;
    }

    const resumeClone = resumeContainer.cloneNode(true);
    const printWindow = window.open("", "_blank");
    printWindow.document.write(`
      <!DOCTYPE html>
      <html>
      <head>
        <title>Resume</title>
        <style>
          @page { margin: 0; size: A4; }
          body { margin: 0; padding: 0; background: white; }
          .print-container {
            box-sizing: border-box;
            padding: 20px;
            width: 100%;
            height: 100%;
            page-break-after: avoid;
            page-break-inside: avoid;
          }
          ${Array.from(document.styleSheets)
            .map((sheet) => {
              try {
                return Array.from(sheet.cssRules)
                  .map((rule) => rule.cssText)
                  .join("\n");
              } catch (e) {
                return "";
              }
            })
            .join("\n")}
        </style>
      </head>
      <body>
        <div class="print-container"></div>
      </body>
      </html>
    `);

    printWindow.document
      .querySelector(".print-container")
      .appendChild(resumeClone);
    printWindow.document.close();

    printWindow.onload = () => {
      setTimeout(() => {
        printWindow.print();
        printWindow.onafterprint = () => printWindow.close();
      }, 500);
    };
  }

  // Add/Delete helpers
  const addItem = (list, template) =>
    updateResume(() => editableResume[list].push(template));
  const deleteItem = (list, idx) =>
    updateResume(() => editableResume[list].splice(idx, 1));
  const addResponsibility = (i) =>
    updateResume(() => editableResume.employment[i].responsibilities.push(""));
  const deleteResponsibility = (i, j) =>
    updateResume(() =>
      editableResume.employment[i].responsibilities.splice(j, 1),
    );

  function handleKeydown(event) {
    if ((event.ctrlKey || event.metaKey) && event.key === "s") {
      event.preventDefault();
      if (hasChanges) saveResume();
    }
  }

  onMount(() => {
    window.addEventListener("keydown", handleKeydown);
    return () => window.removeEventListener("keydown", handleKeydown);
  });
</script>

