<script>
    import { Button } from "$lib/components/ui/button";
    import {
        Card,
        CardContent,
        CardDescription,
        CardFooter,
        CardHeader,
        CardTitle,
    } from "$lib/components/ui/card";
    import {
        Upload,
        LoaderCircle,
        FileText,
        Mail,
        Sparkle,
        Settings,
        Plus,
        Trash,
        ChevronRight,
    } from "@lucide/svelte";
    import { Input } from "$lib/components/ui/input";
    import { Label } from "$lib/components/ui/label";
    import {
        user,
        credits,
        updateUserCredits,
        fetchUserCredits,
    } from "$lib/stores/authStore";
    import { onMount } from "svelte";
    import { customizeResume } from "$lib/services/resumeApi";
    import { supabase } from "$lib/supabase";
    import { Textarea } from "$lib/components/ui/textarea";
    import { Slider } from "$lib/components/ui/slider";
    import {
        Dialog,
        DialogContent,
        DialogDescription,
        DialogFooter,
        DialogHeader,
        DialogTitle,
        DialogTrigger,
    } from "$lib/components/ui/dialog";

    let resumeFile = null;
    let resumeFileName = "";
    let jobTitle = "";
    let jobDescription = "";
    let accuracy = [50];
    let isProcessing = false;
    let errorMessage = "";
    let uploadError = "";
    let recentActivity = [];
    let showNewResumeDialog = false;
    let newResumeTitle = "";
    let newResumeJobTitle = "";
    let newResumeError = "";
    let deleteDialogOpen = false;
    let activityToDelete = null;
    let showInsufficientCreditsDialog = false;
    let RESUME_GENERATION_COST = 10;
    let isCreatingNewResume = false;

    onMount(async () => {
        if ($user) {
            const { data, error } = await supabase
                .from("activities")
                .select("id, date, title, job_title, status")
                .eq("user_id", $user.id)
                .order("date", { ascending: false });
            if (error) {
                console.error("Error fetching activities:", error);
            } else {
                recentActivity = data.map((item) => ({
                    id: item.id,
                    date: item.date,
                    title: item.title,
                    jobTitle: item.job_title,
                    status: item.status,
                }));
            }
            await fetchUserCredits($user.id);
        }
    });

    function handleFileChange(event) {
        const files = event.target.files;
        if (files.length > 0) {
            resumeFile = files[0];
            resumeFileName = resumeFile.name;
            uploadError = "";
            if (!resumeFile.type.includes("pdf")) {
                uploadError = "Please upload a PDF file.";
                resumeFile = null;
                resumeFileName = "";
            }
        }
    }

    async function handleSubmit() {
        if (!resumeFile) {
            uploadError = "Please upload your resume first.";
            return;
        }
        if (!jobDescription.trim() || !jobTitle.trim()) {
            errorMessage = "Please enter both job title and description.";
            return;
        }

        // Check if user has enough credits
        if ($credits < RESUME_GENERATION_COST) {
            showInsufficientCreditsDialog = true;
            return;
        }

        try {
            isProcessing = true;
            errorMessage = "";

            // Deduct credits first
            const creditUpdateSuccess = await updateUserCredits(
                -RESUME_GENERATION_COST,
                `Resume generation: ${jobTitle}`,
            );

            if (!creditUpdateSuccess) {
                errorMessage = "Failed to process credits. Please try again.";
                isProcessing = false;
                return;
            }

            const result = await customizeResume(
                resumeFile,
                jobDescription,
                accuracy[0],
            );

            const today = new Date().toISOString().split("T")[0];

            const { data: insertData, error: insertError } = await supabase
                .from("activities")
                .insert([
                    {
                        user_id: $user.id,
                        date: today,
                        title: resumeFileName.replace(".pdf", ""),
                        job_title: jobTitle,
                        status: "completed",
                        result: result,
                    },
                ])
                .select();

            if (insertError) {
                console.error("Error saving activity:", insertError);
            } else if (insertData && insertData.length > 0) {
                const newActivity = {
                    id: insertData[0].id,
                    date: today,
                    title: resumeFileName.replace(".pdf", ""),
                    jobTitle: jobTitle,
                    status: "completed",
                };

                recentActivity = [newActivity, ...recentActivity];
            }

            resumeFile = null;
            resumeFileName = "";
            jobTitle = "";
            jobDescription = "";
        } catch (error) {
            console.error("Error processing resume:", error);
            errorMessage =
                "–ü—Ä–æ–∏–∑–æ—à–ª–∞ –æ—à–∏–±–∫–∞ –ø—Ä–∏ –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏ —Ä–µ–∑—é–º–µ. –¢–æ–∫–µ–Ω—ã –≤–µ—Ä–Ω—É–ª–∏ –Ω–∞ –≤–∞—à –±–∞–ª–∞–Ω—Å. –ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â–µ —Ä–∞–∑.";

            // Refund the credits if the generation fails
            await updateUserCredits(
                RESUME_GENERATION_COST,
                `Refund for failed resume generation: ${jobTitle}`,
            );
        } finally {
            isProcessing = false;
        }
    }

    async function createNewResume() {
        if (!newResumeTitle.trim() || !newResumeJobTitle.trim()) {
            newResumeError = "–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ —Ä–µ–∑—é–º–µ –∏ –¥–æ–ª–∂–Ω–æ—Å—Ç—å.";
            return;
        }

        try {
            isCreatingNewResume = true;
            newResumeError = "";

            // –ë–∞–∑–æ–≤—ã–π —à–∞–±–ª–æ–Ω —Ä–µ–∑—é–º–µ
            const emptyResume = {
                personal_info: {
                    name: "",
                    email: "",
                    phone: "",
                    linkedin: "",
                    github: "",
                    location: "",
                },
                education: [
                    {
                        institution: "",
                        degree: "",
                        duration: "",
                        location: "",
                    },
                ],
                employment: [
                    {
                        title: "",
                        company: "",
                        duration: "",
                        responsibilities: [""],
                    },
                ],
                projects: [
                    {
                        title: "",
                        url: "",
                        description: "",
                    },
                ],
                technical_skills: [""],
            };

            const today = new Date().toISOString().split("T")[0];

            const { data: insertData, error: insertError } = await supabase
                .from("activities")
                .insert([
                    {
                        user_id: $user.id,
                        date: today,
                        title: newResumeTitle,
                        job_title: newResumeJobTitle,
                        status: "completed",
                        result: emptyResume,
                    },
                ])
                .select();

            if (insertError) {
                console.error("Error creating new resume:", insertError);
                newResumeError =
                    "–û—à–∏–±–∫–∞ –ø—Ä–∏ —Å–æ–∑–¥–∞–Ω–∏–∏ —Ä–µ–∑—é–º–µ. –ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞.";
            } else if (insertData && insertData.length > 0) {
                // –î–æ–±–∞–≤–ª—è–µ–º –≤ —Å–ø–∏—Å–æ–∫ –∞–∫—Ç–∏–≤–Ω–æ—Å—Ç–µ–π
                const newActivity = {
                    id: insertData[0].id,
                    date: today,
                    title: newResumeTitle,
                    jobTitle: newResumeJobTitle,
                    status: "completed",
                };

                recentActivity = [newActivity, ...recentActivity];

                // –ü–µ—Ä–µ–Ω–∞–ø—Ä–∞–≤–ª—è–µ–º –Ω–∞ —Å—Ç—Ä–∞–Ω–∏—Ü—É —Ä–µ–¥–∞–∫—Ç–∏—Ä–æ–≤–∞–Ω–∏—è
                window.location.href = `/dashboard/resume/${insertData[0].id}`;
            }

            // –°–±—Ä–æ—Å–∏—Ç—å –ø–æ–ª—è –∏ –∑–∞–∫—Ä—ã—Ç—å –¥–∏–∞–ª–æ–≥
            newResumeTitle = "";
            newResumeJobTitle = "";
            showNewResumeDialog = false;
        } catch (error) {
            console.error("Error creating resume:", error);
            newResumeError =
                "–û—à–∏–±–∫–∞ –ø—Ä–∏ —Å–æ–∑–¥–∞–Ω–∏–∏ —Ä–µ–∑—é–º–µ. –ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞.";
        } finally {
            isCreatingNewResume = false;
        }
    }

    async function deleteResume() {
        if (!activityToDelete) return;

        try {
            const { error } = await supabase
                .from("activities")
                .delete()
                .eq("id", activityToDelete.id);

            if (error) {
                console.error("Error deleting resume:", error);
                return;
            }

            // –û–±–Ω–æ–≤–ª—è–µ–º —Å–ø–∏—Å–æ–∫ –∞–∫—Ç–∏–≤–Ω–æ—Å—Ç–µ–π, –∏—Å–∫–ª—é—á–∞—è —É–¥–∞–ª–µ–Ω–Ω—É—é
            recentActivity = recentActivity.filter(
                (activity) => activity.id !== activityToDelete.id,
            );

            // –ó–∞–∫—Ä—ã–≤–∞–µ–º –¥–∏–∞–ª–æ–≥
            deleteDialogOpen = false;
            activityToDelete = null;
        } catch (error) {
            console.error("Error in delete operation:", error);
        }
    }

    // –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è –¥–∏–∞–ª–æ–≥–∞ —É–¥–∞–ª–µ–Ω–∏—è
    function confirmDelete(activity, event) {
        event.preventDefault();
        event.stopPropagation();
        activityToDelete = activity;
        deleteDialogOpen = true;
    }
</script>

