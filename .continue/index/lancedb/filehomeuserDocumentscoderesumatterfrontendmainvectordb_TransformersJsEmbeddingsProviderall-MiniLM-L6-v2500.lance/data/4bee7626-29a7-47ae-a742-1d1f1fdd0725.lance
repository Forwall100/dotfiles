<script>
  import { supabase } from "$lib/supabase";
  import ClassicCV from "$lib/components/classicCV.svelte";
  import BlueCV from "$lib/components/blueCV.svelte";
  import ModernCV from "$lib/components/modernCV.svelte";
  import { onMount } from "svelte";
  import { Button } from "$lib/components/ui/button";
  import { fade } from "svelte/transition";
  import {
    Card,
    CardHeader,
    CardContent,
    CardFooter,
    CardTitle,
  } from "$lib/components/ui/card";
  import { Input } from "$lib/components/ui/input";
